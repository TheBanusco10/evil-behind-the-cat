#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/david/Documents/Juegos/js13k-jam/node_modules/.pnpm/livereload@0.9.3/node_modules/livereload/bin/node_modules:/Users/david/Documents/Juegos/js13k-jam/node_modules/.pnpm/livereload@0.9.3/node_modules/livereload/node_modules:/Users/david/Documents/Juegos/js13k-jam/node_modules/.pnpm/livereload@0.9.3/node_modules:/Users/david/Documents/Juegos/js13k-jam/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/david/Documents/Juegos/js13k-jam/node_modules/.pnpm/livereload@0.9.3/node_modules/livereload/bin/node_modules:/Users/david/Documents/Juegos/js13k-jam/node_modules/.pnpm/livereload@0.9.3/node_modules/livereload/node_modules:/Users/david/Documents/Juegos/js13k-jam/node_modules/.pnpm/livereload@0.9.3/node_modules:/Users/david/Documents/Juegos/js13k-jam/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../livereload/bin/livereload.js" "$@"
else
  exec node  "$basedir/../livereload/bin/livereload.js" "$@"
fi
